    "husky": "^4.2.5",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^2.1.6",
    "proxyquire": "1.7.4",
    "request-promise": "^4.2.4",
    "sass-loader": "^7.3.1",
    "sgmf-scripts": "^2.3.0",
    "sinon": "^1.17.7",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-scss": "^2.5.0"
  },
  "browserslist": [
